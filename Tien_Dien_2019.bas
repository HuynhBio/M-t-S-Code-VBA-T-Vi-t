Attribute VB_Name = "Tien_Dien_2019"
Public SOTIEN
Sub TIEN(MUC, TOIDA, DONGIA)
	If MUC < 0 Then
		SOTIEN = 0
	ElseIf MUC < TOIDA Then
		SOTIEN = MUC * DONGIA
	Else
		SOTIEN = TOIDA * DONGIA
	End If
End Sub
Function TIENDIEN(kWh)
	Dim MUC0, MUC1, MUC2, MUC3, MUC4, MUC5
	Dim TIEN0, TIEN1, TIEN2, TIEN3, TIEN4, TIEN5
	MUC0 = kWh
	MUC1 = MUC0 - 50
	MUC2 = MUC1 - 50
	MUC3 = MUC2 - 100
	MUC4 = MUC3 - 100
	MUC5 = MUC4 - 100
	Call TIEN(MUC0, 50, 1678)
	TIEN0 = SOTIEN
	Call TIEN(MUC1, 50, 1734)
	TIEN1 = SOTIEN
	Call TIEN(MUC2, 50, 2014)
	TIEN2 = SOTIEN
	Call TIEN(MUC3, 50, 2536)
	TIEN3 = SOTIEN
	Call TIEN(MUC4, 50, 2834)
	TIEN4 = SOTIEN
	If MUC5 > 0 Then
		TIEN5 = MUC5 * 2927
	Else
		TIEN5 = 0
	End If
	TIENDIEN = (TIEN0 + TIEN1 + TIEN2 + TIEN3 + TIEN4 + TIEN5) * 1.1 '10% THUE GTGT
End Function
